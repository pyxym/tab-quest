    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.21",
    "plasmo": "^0.90.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "tabs",
      "storage",
      "activeTab",
